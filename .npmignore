# .npmignore - Files to exclude from npm package

# Development files
.git
.gitignore
.env
.env.local
.DS_Store

# Python bytecode
__pycache__/
*.py[cod]
*$py.class
*.so

# Data directories (don't ship user data)
nexadb_data/
nexadb_wal/
*.wal
*.sst
*.log

# Test files
tests/
test/
*.test.js
*.spec.js

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Build artifacts
build/
dist/
*.egg-info/

# Documentation drafts
docs/drafts/
*.draft.md

# CI/CD
.github/
.gitlab-ci.yml
.travis.yml

# Backup files
*.backup
*.bak
*.old

# OS files
Thumbs.db
desktop.ini

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Node modules (don't ship dependencies)
node_modules/
